<h2>Show Merchant</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @merchant.name %>
  </li>

  <li>
    <strong>Email:</strong>
    <%= @merchant.credential.email %>
  </li>

</ul>

<span><%= link "Edit", to: merchant_path(@conn, :edit, @merchant) %></span>
<span><%= link "Back", to: merchant_path(@conn, :index) %></span>
